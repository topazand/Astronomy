/*!
 * fancyBox - bottom
 */

$(".togglebox").hide();
$("h2").eq(1).addClass("active");
$(".togglebox").eq(1).show();

$("h2").click(function(){
 $(this).toggleClass("active");
$(this).next(".togglebox").slideToggle("slow");
});



	$("a.tabs").click(function () {
      $("a.tabs").removeClass("active");
      $(this).toggleClass("active", "slow");
    });
	

